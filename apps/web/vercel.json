{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/client",
  "framework": null,
  "rewrites": [
    {
      "source": "/en",
      "destination": "/en/index.html"
    },
    {
      "source": "/zh",
      "destination": "/zh/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/_shell/index.html"
    }
  ]
}
